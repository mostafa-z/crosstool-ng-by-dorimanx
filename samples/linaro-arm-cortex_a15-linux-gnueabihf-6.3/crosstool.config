CT_EXPERIMENTAL=y
CT_LOCAL_TARBALLS_DIR="${CT_TOP_DIR}/src"
CT_SAVE_TARBALLS=y
CT_BUILD_TOP_DIR="${CT_WORK_DIR}/${CT_HOST:+HOST-${CT_HOST}/}${CT_TARGET}"
CT_PREFIX_DIR="${CT_TOP_DIR}/builds/${CT_TARGET}"
CT_PARALLEL_JOBS=0
CT_LOG_EXTRA=y
CT_ARCH_CPU="cortex-a15"
CT_ARCH_FPU="neon-vfpv4"
CT_ARCH_FLOAT_HW=y
CT_ARCH_arm=y
CT_TOOLCHAIN_PKGVERSION="Cortex-A15-hf"
CT_TARGET_VENDOR="cortex_a15"
CT_TARGET_ALIAS="arm-eabi"
CT_KERNEL_linux=y
CT_KERNEL="linux"
CT_KERNEL_V_4_10=y
# CT_LIBC_GLIBC_KERNEL_VERSION_AS_HEADERS is not set
CT_LIBC_GLIBC_KERNEL_VERSION_CHOSEN=y
CT_LIBC_GLIBC_MIN_KERNEL_VERSION="3.4.0"
CT_LIBC_GLIBC_MIN_KERNEL="3.4.0"
CT_BINUTILS_LINKER_LD_GOLD=y
CT_BINUTILS_GOLD_THREADS=y
CT_BINUTILS_LD_WRAPPER=y
CT_BINUTILS_PLUGINS=y
CT_CC_GCC_SHOW_LINARO=y
CT_CC_GCC_V_linaro_6_3=y
CT_CC_GCC_CORE_EXTRA_CONFIG_ARRAY="--with-float=hard"
CT_CC_GCC_EXTRA_CONFIG_ARRAY="--with-float=hard"
CT_CC_LANG_CXX=y
CT_DEBUG_gdb=y
# CT_GDB_CROSS_PYTHON is not set
